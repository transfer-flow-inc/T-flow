{
  "github": {
    "release": true
  },
  "plugins": {
    "@j-ulrich/release-it-regex-bumper": {
      "out": [
        {
          "file": "./sonar-project.properties",
          "search": "sonar.projectVersion=\\d+\\.\\d+\\.\\d+",
          "replace": "sonar.projectVersion={{version}}"
        }
      ]
    }
  }
}

